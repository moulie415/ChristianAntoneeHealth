{
  "expo": {
    "name": "CA Health",
    "slug": "ca-health",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/images/icon.png",
    "scheme": "cahealth",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "bundleIdentifier": "com.HealthAndMovement",
      "entitlements": {
        "aps-environment": "production",
        "com.apple.developer.applesignin": ["Default"],
        "com.apple.developer.associated-domains": [],
        "com.apple.developer.devicecheck.appattest-environment": "production",
        "com.apple.developer.healthkit": true,
        "com.apple.developer.healthkit.access": [],
        "com.apple.developer.healthkit.background-delivery": true,
        "keychain-access-groups": ["$(AppIdentifierPrefix)com.HealthAndMovement"]   
      },
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification",
          "audio",
          "fetch",
          "processing"
        ],
        "BGTaskSchedulerPermittedIdentifiers": [
          "com.transistorsoft.fetch"
        ],
        "SKAdNetworkItems": [
          {
            "SKAdNetworkIdentifier": "v9wttpbfk9.skadnetwork"
          },
          {
            "SKAdNetworkIdentifier": "n38lu8286q.skadnetwork"
          },
          {
            "SKAdNetworkIdentifier": "v9wttpbfk9.skadnetwork"
          },
          {
            "SKAdNetworkIdentifier": "n38lu8286q.skadnetwork"
          }
        ],
        "LSApplicationQueriesSchemes": [
          "mailto",
          "message",
          "readdle-spark",
          "airmail",
          "ms-outlook",
          "googlegmail",
          "inbox-gmail",
          "ymail",
          "superhuman",
          "yandexmail",
          "fastmail",
          "protonmail",
          "mailto",
          "message",
          "readdle-spark",
          "airmail",
          "ms-outlook",
          "googlegmail",
          "inbox-gmail",
          "ymail",
          "superhuman",
          "yandexmail",
          "fastmail",
          "protonmail"
        ],
        "ITSAppUsesNonExemptEncryption": false
      },
      "usesAppleSignIn": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "googleServicesFile": "./google-services.json",
      "package": "com.healthandmovement",
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.DOWNLOAD_WITHOUT_NOTIFICATION",
        "android.permission.ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "image": "./app/images/splash_blank.png"
        }
      ],
      "expo-health-connect",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-firebase/messaging",
      "@react-native-firebase/app-check",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "forceStaticLinking": [
              "RNFBApp",
              "RNFBAuth",
              "RNFBFirestore"
            ]
          },
          "android": {
            "compileSdkVersion": 36,
            "targetSdkVersion": 36,
            "minSdkVersion": 26
          }
        }
      ],
      "@react-native-google-signin/google-signin",
      "react-native-email-link",
      "react-native-compressor",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "health-and-movement",
          "organization": "henry-moule"
        }
      ],
      "expo-audio",
      "@config-plugins/react-native-blob-util",
      "@config-plugins/react-native-pdf",
      "expo-apple-authentication",
      "react-native-background-fetch",
      ["@kingstinct/react-native-healthkit", {
        "NSHealthShareUsageDescription": "Read and understand health data.",
        "NSHealthUpdateUsageDescription": "Track your biometric data within CA Health",
        "background": true
      }]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6b5bcaa9-92d2-473a-bf88-9d84b36dd0b2"
      }
    },
    "owner": "mouleeee"
  }
}
